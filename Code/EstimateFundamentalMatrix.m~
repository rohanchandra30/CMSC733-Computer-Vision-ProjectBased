function F = EstimateFundamentalMatrix(X1,X2)
%% Input
% X1 and X2: Nx2 matrices whose row represents a correspondence
%% Output
% F: 3x3 Fundamental Matrix of Rank 2

%% Your Code goes here
 A = ones(size(X1,1),9);
 for i = 1: size(X1,1)
    x1 = X1(i,1);
    y1 = X1(i,2);
    xx = X2(i,1);
    xy = X3(i,2);
    A(i,:) = [x1*xx x1*yy x1 y1*xx y1*yy y1 xx yy 1]; 
 end
 
 f = svd(A);
 F = f(:9);
 F = reshape(3,3)
end
